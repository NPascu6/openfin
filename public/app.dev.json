{
    "devtools_port": 9090,
    "runtime": {
        "arguments": "--enable-aggressive-domstorage-flushing",
        "version": "11.69.42.14"
    },
    "startup_app": {
        "name": "openfin-demo-app",
        "uuid": "eeb07543-4a39-4fcb-a9cd-cfbb22c32117",
        "url": "http://localhost:3000",
        "applicationIcon": "http://localhost:3000/favicon.ico",
        "clearChildSubscriptionsOnReload": true,
        "autoShow": true,
        "minWidth": 400,
        "defaultWidth": 1200,
        "defaultCentered": true,
        "saveWindowState": true,
        "backgroundColor": "#fffff",
        "minHeight": 400,
        "defaultHeight": 825,
        "cornerRounding": {
            "width": 5,
            "height": 5
        },

        "frame": true,
        "contextMenu": true
    },
    "services": [
        {
            "name": "layouts",
            "config": {
                "features": {
                    "tab": false,
                    "dock": true,
                    "snap": true
                }
            }
        }
    ],
    "shortcut": {
        "company": "",
        "description": "openfin demo app",
        "name": "Openfin demo app",
        "icon": "http://localhost:3000/favicon.ico"
    }
}